url-path: /taxi/
request:
template-source: taxi.sql
connection: 
  - taxi-parquet

auth:
  enabled: true
  type: basic
  from-aws-secretmanager:
    secret-name: flapi_endpoint_users
    secret-table: taxi_auth
    init: |
      CREATE OR REPLACE SECRET flapi_endpoint_users (
        TYPE S3,
        PROVIDER CREDENTIAL_CHAIN
      );
