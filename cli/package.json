{
  "name": "flapii",
  "version": "0.1.0",
  "description": "CLI client for flapi ConfigService",
  "license": "MIT",
  "type": "module",
  "bin": {
    "flapii": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/index.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:integration": "vitest run --config vitest.integration.config.ts"
  },
  "dependencies": {
    "@flapi/shared": "file:./shared",
    "axios": "^1.7.7",
    "axios-retry": "^4.4.2",
    "boxen": "^5.1.2",
    "camelcase": "^8.0.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "colorette": "^2.0.20",
    "commander": "^12.1.0",
    "enquirer": "^2.4.1",
    "figures": "^6.1.0",
    "js-yaml": "^4.1.0",
    "lodash.kebabcase": "^4.1.1",
    "log-symbols": "^6.0.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.5.1",
    "@typescript-eslint/eslint-plugin": "^8.4.1",
    "@typescript-eslint/parser": "^8.4.1",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "execa": "^9.4.0",
    "nock": "^14.0.4",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  }
}
